<div id="wiki-wrapper" class="results">
<div id="head">
  <h1>Wiki List</h1>
</div>
<div id="results">
{{#has_results}}
  <ul id = "wiki-list">
    {{#results}}
    <li>
      <a href="{{base_url}}/{{.}}">{{.}}</a>
    </li>
    {{/results}}
  </ul>
{{/has_results}}

{{#no_results}}
  <p id="no-results">
    There are no registered wikis.
  </p>
{{/no_results}}

</div>
<div id="footer">
  <ul class="actions">
    <li class="minibutton"><a href="#">Back to Top</a></li>
  </ul>
</div>
</div>